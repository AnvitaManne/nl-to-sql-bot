   "source": [
    "## Define Tool with Interrupt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "9fe59cad-6603-4d2f-bf1b-0075e0471750",
   "metadata": {},
   "outputs": [],
   "source": [
    "def schedule_meeting_with_approval(person: str, date: str, time: str):\n",
    "   \"\"\"Schedule a meeting with a person at a specific date and time.\"\"\"\n",
    "\n",
    "   response = interrupt(\n",
    "       f\"Schedule meeting with person='{person}', date='{date}', time='{time}'. Approve or edit?\"\n",
    "   )\n",
    "   \n",
    "   if response[\"type\"] == \"accept\":      \n",
    "       return f\" Meeting confirmed with {person} on {date} at {time}.\"\n",
    "   elif response[\"type\"] == \"edit\":\n",
    "       person = response[\"args\"].get(\"person\", person)\n",
    "       date = response[\"args\"].get(\"date\", date)\n",
    "       time = response[\"args\"].get(\"time\", time)\n",
    "       return f\"Meeting confirmed with {person} on {date} at {time}.\"\n",
    "   else:\n",
    "       return \"Meeting scheduling cancelled or unknown response.\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "c8430a54-3c09-45ca-9c42-51d990ad7b5e",
   "metadata": {},
   "outputs": [],
   "source": [
    "checkpointer = InMemorySaver()\n",
    "\n",
    "agent = create_react_agent(\n",
    "    model=\"openai:gpt-4\",\n",
    "    tools=[schedule_meeting_with_approval],\n",
    "\n",
    "    checkpointer=checkpointer,\n",
    ")"
   ]
  },
